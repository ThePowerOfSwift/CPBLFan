# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'CPBLFan' do

  use_frameworks!

  # Pods for CPBLFan

  #Pods about Swift Language
  pod 'SwifterSwift'

  #Pods about internet request/response/parse
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'Kanna', '~> 2.1.0'

  #Pods about UI
  pod 'Kingfisher'
  pod 'DynamicColor', '~> 3.1.0'
  pod 'PKHUD', '~> 3.0'

  target 'CPBLFanTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CPBLFanUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
